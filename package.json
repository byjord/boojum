{
  "name": "boojum",
  "version": "0.0.3",
  "description": "An easy way to build Shopify Themes",
  "main": "index.js",
  "bin": {
    "boojum": "cli/entry.js"
  },
  "scripts": {
    "clean": "npm run clean:dev",
    "clean:dev": "npm run clean:debug && npm run clean:coverage && npm run clean:db && rimraf compiled",
    "clean:db": "rimraf db.json",
    "clean:debug": "rimraf debug.log",
    "clean:npm": "rimraf node_modules",
    "clean:coverage": "rimraf coverage && rimraf .nyc_output",
    "start": "cross-env NODE_ENV=development babel-node ./lib/app.js",
    "build": "babel lib --out-dir compiled --source-maps",
    "build:test": "cross-env NODE_ENV=development nyc ava ./compiled",
    "test": "cross-env NODE_ENV=development nyc ava",
    "report": "nyc report --reporter=html",
    "test:eslint": "cross-env NODE_ENV=test eslint --ignore-path .gitignore --ignore-pattern *.test.* --ignore-pattern node_modules --ignore-pattern cli",
    "eslint:fix": "cross-env NODE_ENV=test eslint --ignore-path .gitignore --ignore-pattern tests --ignore-pattern node_modules --ignore-pattern lib/cli --fix"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./lib"
          ],
          "alias": {
            "package.json": "./package.json",
            "app": "./lib/app.js"
          }
        }
      ]
    ]
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "require": [
      "babel-core/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "check-coverage": true,
    "per-file": true,
    "all": true
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./lib"
            ],
            "alias": {
              "package.json": "./package.json",
              "app": "./lib/app.js"
            }
          }
        ]
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byjord/boojum.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/byjord/boojum/issues"
  },
  "homepage": "https://github.com/byjord/boojum#readme",
  "dependencies": {
    "Faker": "^0.7.2",
    "abbrev": "^1.1.0",
    "bluebird": "^3.5.0",
    "chalk": "^2.1.0",
    "hexo-fs": "^0.2.1",
    "hexo-log": "^0.2.0",
    "liquid-node": "^3.0.0",
    "lodash": "^4.17.4",
    "lowdb": "^0.17.1",
    "mz": "^2.6.0",
    "nsfw": "^1.0.16",
    "schema-object": "^4.0.11",
    "snyk": "^1.38.1",
    "speakingurl": "^14.0.1",
    "swig": "^1.4.2",
    "swig-extras": "0.0.1",
    "update-notifier": "^2.2.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-shopify": "^17.0.0",
    "eslint-plugin-sort-class-members": "^1.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1"
  },
  "snyk": true
}
